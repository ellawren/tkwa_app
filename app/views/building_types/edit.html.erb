<% provide :title, @building_type.name %>
<%= link_to "Building Types", building_types_path, class: "header-label" %>
<div class="page-header">
	<h1><%= @building_type.name %></h1>
</div>

<%= form_for @building_type do |f| %>

  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Update Building Type", class: "btn btn-primary save-btn" %>
    </div>

<% end %>