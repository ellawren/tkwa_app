<% provide :title, "Edit Globals" %>
<%= link_to "Globals", globals_path, class: "header-label" %>
<div class="page-header">
	<h1><%= @global.year %></h1>
</div>

<%= form_for(@global) do |f| %>

  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

  	<div class="save-row">
        <%= f.submit "Update", class: "btn btn-primary save-btn" %>
    </div>

<% end %>