    <% provide :title, "New Tag" %>
    <%= link_to "Tags", tags_path, class: "header-label" %>
    <div class="page-header">
        <h1>New Tag</h1>
    </div>

<%= form_for @tag do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Tag", class: "btn btn-primary save-btn" %>
    </div>

<% end %>
