<% provide :title, "New Expense Report" %>
<%= link_to"Expense Reports", user_expense_reports_path(@user), class: "header-label" %>
<div class="page-header">
	<h1><%= "New Expense Report" %></h1>
</div>

<%= form_for @expense_report do |f| %>

  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

  	<div class="save-row">
        <%= f.submit "Create Expense Report", class: "btn btn-primary save-btn" %>
    </div>

<% end %>