<% content_for :page_header do %>
  <% provide :title, "Edit Phase" %>
  <div class="page-header">
    <h1><%= @phase.name %></h1>
  </div>
<% end %>

<%= form_for(@phase) do |f| %>
  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

  	<div class="save-row">
	    <%= f.submit "Save Changes", class: "btn btn-primary save-btn" %>
	</div>
<% end %>