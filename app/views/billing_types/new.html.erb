<% provide :title, "Add New Billing Type" %>
<%= link_to "Billing Type", billing_types_path, class: "header-label" %>
<div class="page-header">
	<h1>Add New Billing Type</h1>
</div>

<%= form_for @billing_type do |f| %>

  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

  	<div class="save-row">
        <%= f.submit "Add Billing Type", class: "btn btn-primary save-btn" %>
    </div>

<% end %>