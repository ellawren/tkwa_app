<% provide :title, @pattern_group.name %>
<%= link_to "Pattern Groups", pattern_groups_path, class: "header-label" %>
<div class="page-header">
	<h1><%= @pattern_group.name %></h1>
</div>


<%= form_for @pattern_group do |f| %>

  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

  	<div class="save-row">
        <%= f.submit "Update Pattern Group", class: "btn btn-primary save-btn" %>
    </div>

<% end %>