<% provide :title, "New Petal" %>
<div class="page-header">
    <h1>New Petal</h1>
</div>

<%= form_for @petal do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Petal", class: "btn btn-primary save-btn" %>
    </div>

<% end %>

