<% provide :title, "Key Categories" %>
<%= link_to "Admin Panel", admin_path, class: "header-label" %>
<div class="page-header">
	<h1>Key Categories</h1>
	<%= link_to "Add Key Category", new_key_category_path, class: "btn btn-primary btn-right" %>
</div>

<ul class="link-list">
	<% @key_categories.each do |param| %>
			<li>
				<%= link_to param.name, edit_key_category_path(param.id) %>
			</li>
	<% end %>
</ul>

