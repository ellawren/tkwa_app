   <fieldset>

        <div class="row">
                      
            <div class="span7-5">

                <h3>General Information</h3>   
                <br />
                
                <div class="control-group">
                    <%= f.label(:year, class: "control-label") %>
                    <div class="controls">
                        <%= f.text_field :year, class: "span1" %>
                    </div>
                </div>

                <div class="control-group">
                    <%= f.label :week, class: "control-label" %>
                    <div class="controls">
                        <%= f.text_field :week, class: "span1" %>
                    </div>
                </div>

                <div class="control-group">
                    <%= f.label :hours, class: "control-label" %>
                    <div class="controls">
                        <%= f.text_field :hours, class: "span1" %>
                    </div>
                </div>

                <div class="control-group">
                    <%= f.label :phase, class: "control-label" %>
                    <div class="controls">
                        <%= f.text_field :phase, class: "span3" %>
                    </div>
                </div>

                <div class="control-group">
                    <%= f.label :task, class: "control-label" %>
                    <div class="controls">
                        <%= f.text_field :task, class: "span3" %>
                    </div>
                </div>

                
                <br />

            </div>
          
        </div> <!-- end row --> 

    </fieldset>
