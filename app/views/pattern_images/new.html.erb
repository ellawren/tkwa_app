<% provide :title, "New Pattern Image" %>
<div class="page-header">
    <h1>New Pattern Image</h1>
</div>

<%= form_for @pattern_image do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Pattern Image", class: "btn btn-primary save-btn" %>
    </div>

<% end %>

