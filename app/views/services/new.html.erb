<% content_for :page_header do %>
    <% provide :title, "Add New Service" %>
    <%= link_to "Services", services_path, class: "header-label" %>
    <div class="page-header">
      	<h1>Add New Service</h1>
    </div>
<% end %>

<%= form_for(@service) do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

  	<div class="save-row">
        <%= f.submit "Add Service", class: "btn btn-primary save-btn" %>
    </div>
    
<% end %>