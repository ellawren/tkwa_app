<% content_for :page_header do %>
  <% provide :title, "Add New Plan Entry" %>
  <div class="page-header">
    <h1>Add New Plan Entry</h1>
  </div>
<% end %>

<%= form_for @plan_entry do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= render 'fields', f: f %>

  <div class="actions">
    <%= f.submit "Create Plan Entry", class: "btn btn-large" %>
  </div>
<% end %>
