var phase = <%= @phase_number %>;
if(phase == 70) {
	$("#timesheet_time_entries_attributes_<%= @key %>_task").parent(".task-container").html("<input class=\"task-writein\" id=\"timesheet_time_entries_attributes_<%= @key %>_task\" name=\"timesheet[time_entries_attributes][<%= @key %>][task]\" size=\"30\" type=\"text\">");
} else {
	$("#timesheet_time_entries_attributes_<%= @key %>_task").parent(".task-container").html("<select class=\"task custom-select\" id=\"timesheet_time_entries_attributes_<%= @key %>_task\" name=\"timesheet[time_entries_attributes][<%= @key %>][task]\"><%= escape_javascript(options_for_select(@tasks)) %></select>");
}
