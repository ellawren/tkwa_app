<% content_for :content do %>
	<% provide :title, @project.name %>
	<div class="page-header">
		<h1><span class="lightweight"><%= number_with_precision(@project.number, :strip_insignificant_zeros => true) %></span> &nbsp;<%= @project.name %></h1>
	</div>

	<%= render "tabnav" %>

	<div class="row">
		<div class="span14 sh">
			<div class="row">	
				<div class="span13 offset05">
  					<%= content_for?(:projects_content) ? yield(:projects_content) : yield %>
  				</div> <!-- end span13 -->
			</div> <!-- end row -->	
  		</div> <!-- end span14 -->
	</div> <!-- end row -->	
<% end %>
<%= render :template => 'layouts/application' %>