<style>
@import url("https://fonts.googleapis.com/css?family=Lato:100,300,400,700,900,100italic,300italic,400italic,700italic,900italic|Asap:400italic,700italic");

body { width:915px; margin:50px; color: #34495e; font-family: "Lato", sans-serif !important; font-size: 14px; line-height: 1.231; }
.page-header { border-bottom: none; margin-bottom:30px; width: 100%; float: left; height: 38px; margin-top:60px; }
.page-header h1 { font-size: 28px; color: #16a085; display: inline-block; width: 700px; margin: 0px; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; font-weight: 900; }
a { text-decoration:none; }
.table-main.timesheets     { float:left; width:100%; margin:0px 0 0 0; }
    .table-main.timesheets.non-billable     { margin:20px 0 20px 0; }
    .table-main.timesheets .table-head { border-bottom:2px solid #333; }

    .table-body     { float:left; width:100%;}
    .table-row      { float:left; width:935px; padding:4px 0 0 0; }

    .table-head { padding-left:0px; border-bottom:2px solid #333;  }
	.chzn-select, .chzn-container { float:left; }

    .timesheets .table-head     	{ float:left; width:100%; padding:0px; border-bottom:1px solid #555; font-size:13px; font-weight:900; }
	.timesheets .table-head .cell 	{ float:left; padding-top:5px; margin:0px 2px 0px 2px; }
	.timesheets .table-body .table-row { padding:0px 0px 0px 8px; border-bottom:1px solid #e1e6eb;}
		.timesheets .table-body .table-row.holiday { background-color:#e1f0f3; width:932px; padding-top:2px; }
		.timesheets .table-body .table-row:first-child.holiday { margin-top:0px; }
		.timesheets .table-body .table-row.holiday .entry-spacer { background-color:#fff; border-color:#b3dbe4; }
		.timesheets .table-body .table-row.holiday .category-cell, .timesheets .table-body .table-row.holiday .description-cell, 
		.timesheets .table-body .table-row.holiday .entry-spacer
				{ color:#0ba0d4; }
	.timesheets .table-body .table-row:first-child { margin-top:4px; }
	.timesheets .custom-select { padding:4px 3px; }
	.timesheets .delete { margin:8px 0 8px 6px; }
	.timesheets.non-billable .table-head     	{ float:left; }

 	/* project */
 	.timesheet-tab { width:130px; margin-top:16px; font-weight:900; }
 	.timesheets .table-head .project-name { width:232px; margin:22px 4px 0 0; }
	.timesheets .project-name.custom-select  { width:230px; margin:2px 4px 2px -8px; padding: 4px 30px 4px 5px;  /* extra padding at right is to prevent longer project names from overflowing over the down arrow */ }

	.timesheets .table-body .project-cell { width:226px; float:left; padding:8px 0 8px 6px; border-radius:8px; margin:4px 6px 0 -10px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; }
		.timesheets .table-body .project-cell a { color:#1abc9c; }
		.timesheets .table-body .project-cell a:hover { color:#16a085; }

 	/* phase */
 	.timesheets .table-head .phase { width:58px; margin-top:22px; }
	.timesheets .table-body .phase.custom-select { width:60px; margin:2px 4px 2px 0; }
		.timesheets .table-body .phase-cell { width:39px; float:left; padding:8px 0; margin:4px 25px 4px 0px; text-align:center; color:#1abc9c; }

	/* task */
	.timesheets .table-head .task { width:184px; margin-top:22px; }
	.timesheets .table-body .task.custom-select { width:186px; margin:2px 4px 2px 0; padding: 4px 30px 4px 5px; }
		.timesheets .table-body .task-cell { width:178px; float:left; padding:8px 0 8px 6px; margin:4px 5px 4px 0px; color:#1abc9c; overflow:hidden; text-overflow:ellipsis; white-space:nowrap; }


	/* time entries */
	.timesheets .table-head .date { float:left; width:41px; padding:4px 4px 0 4px; font-size:11px; font-weight:400; text-align:center; text-transform:uppercase; }
	.timesheets .table-head .date.today { color:#f1c40f; }
	.timesheets .table-head .date .blanch { width:40px; padding:4px 0 0 0; margin:0px; font-size:15px; font-weight:900; text-align:center; }
	.timesheets .time-entries { float:left; height:36px;}
 	.timesheets .time-entries .entry { float:left; }
 	.timesheets .time-entries .text-input { width:39px; height:32px; padding:0px 3px; margin:2px 4px 2px 0; text-align:center; }
 		 	.timesheets .saved .editable .time-entries .text-input, .timesheets .saved .editable .custom-select { border-color:#1abc9c; }
			.timesheets .saved .editable .time-entries .text-input:focus { border-color:#16a085; }
 	.timesheets .time-entries .text-input.weekend { background-color:#f4f6f9; }
	.timesheets .time-entries .entry.today .text-input { border-color:#f1c40f; }
	.timesheets .time-entries .entry.today .text-input:focus { border-color:#5bae60; }

	/* uneditable time entries */
	.timesheets .time-entries .entry .entry-spacer { width:39px; height:16px; padding:6px 3px 10px; margin:2px 4px 2px 0; text-align:center; border: 2px solid #f4f6f9; color: #16a085; font-size: 14px; text-indent: 0px; font-family: "Lato", sans-serif; -webkit-appearance: none; -webkit-border-radius: 6px; -moz-border-radius: 6px; border-radius: 6px; display: inline-block; line-height: 20px; vertical-align: middle;}
	.timesheets .time-entries .entry .entry-spacer.weekend { background-color:#f4f6f9; }
	.timesheets .time-entries .entry.today .entry-spacer { border-color:#f1c40f; }

 	/* total */
 	.timesheets .table-head .total { float:left; width:48px; padding:0px 4px; margin:26px 0 0 0; text-align:center; }
	.timesheets .table-body .total { float:left; width:40px; height:21px; padding:12px 0 6px 5px; margin:0px 4px 0px 0; font-size:13px; font-weight:900; text-align:center; }

	/* nb - category */
	.timesheets .table-head .category { width:198px; padding-left:0px; margin-top:0px; margin-left:0px; }
	.timesheets .category.custom-select { width:208px; margin:2px 4px 2px -8px; }
		.timesheets .table-body .category-cell { width:204px; float:left; padding:8px 0 8px 6px; margin:4px 5px 4px -10px; color:#1abc9c; }

	/* nb - description */
	.timesheets .table-head .nb-description { float:left; width:270px; padding-left:12px; margin-top:17px; }
	.text-input.nb-description { float:left; width:248px; height:32px; margin:2px 4px 2px 0; text-align:left; }
		.timesheets .table-body .description-cell { width:264px; float:left; padding:8px 0 8px 6px; margin:4px 5px 4px 0px; color:#1abc9c; }

	/* add link */
  	.add-link       { float:left; display:block; padding:2px 0 0 26px; margin-top:3px; background:url(/assets/add-mini-tall.png); background-repeat:no-repeat; font-family:Argumentum-Medium; text-decoration:none; }
  	.add-link-sm    { float:left; display:block; height:25px; padding:3px 0 0 26px; margin-top:3px; background:url(/assets/add-mini.png); background-repeat:no-repeat; font-family:Argumentum-Medium; text-decoration:none; }
  	.table-main .add-link   { width:93%; height:20px; padding-top:10px; padding-left:32px; margin-top:-1px; border-top:1px solid #555; background-position:8px 5px; }

  	/* footer */
    .table-footer     { float:left; width:101%; margin:0px; border-top:2px solid #000; }
    	.table-footer .span { width:453px; margin-left:492px; }
    		.table-footer .sum { float:left; width:30px; height:22px; padding:7px 9px 4px 9px; margin:2px 3px 2px 2px; font-size:13px; font-weight:400; text-align:right; }
			.table-footer .total { float:left; width:45px; height:20px; padding:10px 5px 6px 5px; margin:0 3px 0 4px; font-size:13px; font-weight:900; text-align:center; }

.page-header.print { margin-top:60px; }
.print .timesheets { width:900px; }

.print .timesheets .table-body .project-cell, .print .timesheets .table-body .phase-cell, .print .timesheets .table-body .task-cell,
.print .timesheets .table-body .category-cell, .print .timesheets .table-body .description-cell  
	{ padding-top:6px; padding-bottom:5px; margin-top:0px; }
.print .timesheets .time-entries { height:30px; }

.print .timesheets .table-head .project-name { width:222px; }
.print .timesheets .table-body .project-cell { width:216px; }

.print .timesheets .table-body .task-cell { width:168px; }
.print .timesheets .table-head .task { width:174px; }

.print .timesheets .table-body .category-cell { width:184px; }
.print .timesheets .table-head .category { width:188px; }

.print .timesheets .time-entries .entry .entry-spacer { padding-top:3px; padding-bottom:7px; border:none; width:43px; margin-top:3px; }
.print .timesheets .time-entries .entry .entry-spacer.weekend { background:none; }
.print .timesheets .table-body .total { padding-top:8px; padding-bottom:3px; }

.print .timesheets .table-body .table-row.holiday { background:none; }
.print .timesheets .table-body .table-row { border-bottom: 1px solid #ced5da; width:892px; }

.print .timesheets.total .table-body { border:2px solid #34495e; margin-top:20px; color:#34495e; font-weight:700; margin-bottom:10px; }
.print .timesheets.total .table-body .table-row:first-child { margin-top:0px; }
.print .timesheets.total .table-body .table-row { border-bottom:none; }
.print .timesheets.total .table-body .category-cell { padding-left:10px; color:#34495e; width:178px; margin-top:2px; margin-bottom:2px; }
.print .timesheets.total .time-entries .entry .entry-spacer { color:#34495e; }


</style>
<div class="modal-dialog">
	<div class="modal-content">
  		
  		<%= content_for?(:content) ? yield(:content) : yield %>

	</div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->