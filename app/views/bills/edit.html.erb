

<div class="header-span">
    <h3>Bills</h3>
</div>

<div class="body-span">
	<%= f.fields_for :bills do |f| %>

		<div class="span">
		    <%= f.label(:date, class: "label") %>
		    <%= f.text_field :date, class: "input span6" %>
		</div>
		<div class="span">
		   <%= f.label(:amount, class: "label") %>
		   <%= f.text_field :amount, class: "input span6" %>
		</div>

	<% end %>
</div>



