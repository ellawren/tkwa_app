<% provide :title, "New Category" %>
<%= link_to "Categories", categories_path, class: "header-label" %>
<div class="page-header">
    <h1>New Category</h1>
</div>

<%= form_for @category do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Category", class: "btn btn-primary save-btn" %>
    </div>

<% end %>

