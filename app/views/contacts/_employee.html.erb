<li>
	<% if f.employee %>
		<% user = User.find(f.employee.user_id) %>
		<% employee = f.employee %>
	<% end %>
    <label class="checkbox">
    	<input type='checkbox' class='ck' value='<%= f.work_email %>'>
    </label>
	
	<div class="photo"><%= image_tag user.photo.url(:thumb) if user %></div>
    <div class="name"><%= link_to f.name, contact_path(f) %></div>
    <div class="ext"><%= f.work_ext %></div>
    <div class="cell"><%= number_to_phone(f.cell_phone, :area_code => true) %></div>
    <div class="email"><a class="button mail-link" onclick="mailTo('<%= f.work_email %>');"><%= f.work_email %></a></div>
    <div class="start"><%= employee.hired if f.employee %></div>
	<div class="bday"><%= employee.birthday if f.employee %></div>
    <div class="link"><% if f.employee %><%= link_to "timesheets", all_timesheets_path(user.id), class: "btn" %><% end %></div>

</li>
