<% provide :title, "New Living Building Version" %>
<div class="page-header">
    <h1>New Living Building Version</h1>
</div>

<%= form_for @living_building do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Living Building Version", class: "btn btn-primary save-btn" %>
    </div>

<% end %>

