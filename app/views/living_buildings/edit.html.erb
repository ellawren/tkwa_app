<% provide :title, @living_building.version %>
<div class="page-header">
    <h1><%= @living_building.name %></h1>
</div>

<%= form_for @living_building do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Save", class: "btn btn-primary save-btn" %>
    </div>

<% end %>