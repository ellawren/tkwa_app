<% provide :title, "Living Buildings" %>
<%= link_to "Admin Panel", admin_path, class: "header-label" %>
<div class="page-header">
	<h1>Living Buildings</h1>
	<%= link_to "Add Living Building Version", new_living_building_path, class: "btn btn-primary btn-right" %>
</div>

<ul class="link-list">
	<% @living_buildings.each do |param| %>
			<li>
				<%= link_to param.version, edit_living_building_path(param.id) %>
				<% if signed_in? %>
					<% if current_user.admin? %>
					| <%= link_to "delete", param, method: :delete, confirm: "You sure?" %>
					<% end %>
				<% end %>
			</li>
	<% end %>
</ul>

