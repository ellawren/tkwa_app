<% provide :title, "Living Buildings" %>

<div class="page-header">
    <h1>Living Building Strategy Group</h1>
</div>

<%= form_for(@lb_strategy_group) do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Update Strategy Group", class: "btn btn-primary save-btn" %>
    </div>

<% end %>