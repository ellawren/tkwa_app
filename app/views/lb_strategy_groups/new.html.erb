<% provide :title, "New Strategy Group" %>
<div class="page-header">
    <h1>New Strategy Group</h1>
</div>

<%= form_for @lb_strategy_group do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Strategy Group", class: "btn btn-primary save-btn" %>
    </div>

<% end %>

