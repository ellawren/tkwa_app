<% content_for :page_header do %>
  <% provide :title, @consultant_role.consultant_role_name %>
  <div class="page-header">
    <h1><%= @consultant_role.consultant_role_name %></h1>
  </div>
<% end %>

<%= form_for @consultant_role do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= render 'fields', f: f %>

  <div class="actions">
    <%= f.submit "Update Consultant Role", class: "btn" %>
  </div>
<% end %>