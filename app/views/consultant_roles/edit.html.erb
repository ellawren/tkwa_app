<% provide :title, @consultant_role.consultant_role_name %>
<%= link_to "Consultant Roles", consultant_roles_path, class: "header-label" %>
<div class="page-header">
	<h1><%= @consultant_role.consultant_role_name %></h1>
</div>

<%= form_for @consultant_role do |f| %>

  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Update Consultant Role", class: "btn btn-primary save-btn" %>
    </div>

<% end %>