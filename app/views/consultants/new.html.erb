<% provide :title, "New Consultant" %>
<%= link_to "Consultants", consultants_path, class: "header-label" %>
<div class="page-header">
    <h1>New <%= params[:category] == '2' ? "Contractor " : "Consultant" %></h1>
</div>

<%= form_for @consultant do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Create Consultant", class: "btn btn-primary save-btn" %>
    </div>

<% end %>

