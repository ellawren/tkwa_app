<% content_for :page_header do %>
  <% provide :title, "Add New Consultant" %>
  <div class="page-header">
    <h1>Add New Consultant</h1>
  </div>
<% end %>

<%= form_for(@consultant) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= render 'fields', f: f %>

  <div class="actions">
    <%= f.submit "Create Consultant", class: "btn" %>
  </div>
<% end %>
