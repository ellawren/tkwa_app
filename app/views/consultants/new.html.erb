<% provide :title, "New Company" %>
<%= link_to "Companies", consultants_path, class: "header-label" %>
<div class="page-header">
    <h1>New Company</h1>
    
</div>

<%= form_for @consultant do |f| %>

    <%= render 'fields', :f => f %>

    <div class="save-row">
        <%= f.submit "Create Company", class: "btn btn-primary save-btn" %>
    </div>

<% end %>