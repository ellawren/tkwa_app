<% content_for :page_header do %>
  	<% provide :title, @consultant.company %>
  	<div class="page-header">
    	<h1><%= @consultant.company %></h1>
  	</div>
<% end %>

<%= form_for(@consultant) do |f| %>
  	<%= render 'shared/error_messages', object: f.object %>
  	<%= render 'fields', f: f %>

 	<div class="save-row">
        <%= f.submit "Update Consultant", class: "btn btn-primary save-btn" %>
    </div>
<% end %>