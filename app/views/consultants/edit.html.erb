<% content_for :page_header do %>
  <% provide :title, "Edit Consultant" %>
  <div class="page-header">
    <h1><%= @consultant.company %></h1>
  </div>
<% end %>

<%= form_for(@consultant) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= render 'fields', f: f %>

  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>