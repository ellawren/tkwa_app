<% content_for :page_header do %>
  <% provide :title, "Add New Reimbursable" %>
  <div class="page-header">
    <h1>Add New Reimbursable</h1>
  </div>
<% end %>

<%= form_for @reimbursable do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= render 'fields', f: f %>

  <div class="actions">
    <%= f.submit "Add Reimbursable", class: "btn" %>
  </div>
<% end %>