<% content_for :page_header do %>
  <% provide :title, @reimbursable.reimbursable_name %>
  <div class="page-header">
    <h1><%= @reimbursable.reimbursable_name %></h1>
  </div>
<% end %>

<%= form_for @reimbursable do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <%= render 'fields', f: f %>

  <div class="actions">
    <%= f.submit "Update Reimbursable", class: "btn" %>
  </div>
<% end %>