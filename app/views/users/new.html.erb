<% content_for :page_header do %>
  	<% provide :title, "New User" %>
  	<%= link_to "Admin Panel", admin_path, class: "header-label" %>
  	<div class="page-header">
    	<h1>Add New User</h1>
  	</div>
<% end %>

<%= form_for(@user, :multipart => true, class: "form-horizontal" ) do |f| %>

    <%= render 'shared/error_messages', object: f.object %>
    <%= render 'fields', f: f %>

    <div class="save-row">
        <%= f.submit "Add User", class: "btn btn-primary save-btn" %>
    </div>

<% end %>